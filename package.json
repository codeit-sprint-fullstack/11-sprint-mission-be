  "license": "ISC",
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "bugs": {
    "url": "https://github.com/codeit-sprint-fullstack/11-sprint-mission-be/issues"
  },
  "homepage": "https://github.com/codeit-sprint-fullstack/11-sprint-mission-be#readme",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "express": "^5.2.1",
    "pg": "^8.17.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
